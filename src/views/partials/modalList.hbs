<div
   class="modal fade"
   id="modalList"
   data-backdrop="static"
   data-keyboard="false"
   tabindex="-1"
   aria-labelledby="staticBackdropLabel"
   aria-hidden="true"
>
   <div class="modal-dialog modal-dialog-scrollable">
      <div class="modal-content">
         <div class="modal-header timeHeader">
            <button type="button" class="btn btn-sm bC" data-dismiss="modal">
               <i class='bx bx-x-circle bx-tada' ></i>
            </button>

            <h5 class="modal-title" id="staticBackdropLabel">Busqueda de boleto</h5>

            <img src="/img/BiingSIO.png" alt="VLP Unesum" />
         </div>

         <div class="modal-body timeBody">
            <form id="formAdd">
               <div class="form-group">
                  <label for="">Apellidos</label>
                  <input type="text" class="form-control" id="apellidosS" placeholder="Ingrese el apellido" disabled>
               </div>

               <div class="form-group">
                  <label for="">Nombres</label>
                  <input type="text" class="form-control" id="nombresS" placeholder="Ingrese el nombre" disabled>
               </div>

               <div class="form-group">
                  <label for="">Teléfono</label>
                  <input type="text" class="form-control" id="telefonoS" placeholder="Ingrese el numero de teléfono" disabled>
               </div>

               <div class="form-group">
                  <label for=""># de Boleto</label>
                  <input type="text" class="form-control" id="numBoletoS" placeholder="Ingrese el numero de boleto" disabled>
               </div>

               <div class="form-group">
                  <label for="">Vendedor</label>
                  <input type="text" class="form-control" id="vendedorS" placeholder="Ingrese el numero del vendedor" disabled>
               </div>
               <hr>
            </form>            
         </div>
      </div>
   </div>
</div>